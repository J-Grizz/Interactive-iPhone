{
  "name": "interactive-iphone",
  "version": "1.0.0",
  "description": "This is my portfolio project which displays my ability with HTHML, pure CSS and JavaScript.",
  "main": "app.html",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10",
    "IE 11",
    "firefox >= 4",
    "safari 7",
    "safari 8"
  ],
  "scripts": {
    "start": "npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "postcss public/stylesheets/*.css -u autoprefixer -d public/build/ -w",
    "serve": "browser-sync start --server --files 'public/scripts/*, public/stylesheets/*, public/build/*, views/index.ejs' --no-inject-changes --index index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/J-Grizz/Iphone-Interactive.git"
  },
  "author": "James Gray",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/J-Grizz/Iphone-Interactive/issues"
  },
  "homepage": "https://github.com/J-Grizz/Iphone-Interactive#readme",
  "dependencies": {
    "ejs": "^2.7.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "browser-sync": "^2.26.7",
    "mime": "^2.4.4",
    "postcss-cli": "^6.1.3",
    "run-all": "^1.0.1"
  }
}