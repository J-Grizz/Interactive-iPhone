  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "browser-sync": "^2.26.7",
    "mime": "^2.4.4",
    "postcss-cli": "^6.1.3",
    "run-all": "^1.0.1"
  }
