{
  "name": "interactive-iphone",
  "version": "1.0.0",
  "description": "This is my portfolio project which displays my ability with HTHML, pure CSS and JavaScript.",
  "main": "app.html",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10",
    "IE 11",
    "firefox >= 4",
    "safari 7",
    "safari 8"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "autoprefix": "postcss stylesheets/*.css -u autoprefixer -d build/ -w",
    "serve": "browser-sync start --server --files 'scripts/*, stylesheets/*, build/*, app.html' --no-inject-changes --index app.html"
  },
